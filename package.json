    "url": "https://github.com/rafaelrcamargo/tailwindcss-expose-colors/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaelrcamargo/tailwindcss-expose-colors.git"
  },
  "license": "Apache-2.0",
  "author": "Rafael R. Camargo",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
